{
    "routes": [
        {
            "route": "/src/*.{html,css}",
            "headers": {
                "cache-control": "max-age=0"
            }
        },
        {
            "route": "/src/hello/*.js",
            "headers": {
                "cache-control": "max-age=120"
            }
        },
        {
            "route": "/api/message/*.js",
            "headers": {
                "cache-control": "max-age=120"
            }
        }
    ]
}